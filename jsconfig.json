{
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": "./src",
    "jsx": "react",
    "paths": {
      "@common/*": ["common/*"],
      "@pages/*": ["pages/*"],
      "@components/*": ["common/components/*"],
      "@images/*": ["common/assets/images/*"],
      "@utils/*": ["common/utils/*"],
      "@hook/*/": ["common/hooks/"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*", "build"]
}
